<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="468.0" prefWidth="979.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mass.ejercicionavegabilidad.ControladorPantallaB">
    <children>
   <TableView fx:id="tablaPacientes" layoutX="11.0" layoutY="275.0" prefHeight="366.0" prefWidth="939.0">
         <columns>
            
             <TableColumn text="id">
               <cellValueFactory>
                  <PropertyValueFactory property="id" />
               </cellValueFactory>
            </TableColumn>
            
            <TableColumn text="nombre">
               <cellValueFactory>
                  <PropertyValueFactory property="nombre" />
               </cellValueFactory>
            </TableColumn>
            
            <TableColumn text="apellido">
               <cellValueFactory>
                  <PropertyValueFactory property="apellido" />
               </cellValueFactory>
            </TableColumn>
            
                  <TableColumn text="dni">
               <cellValueFactory>
                  <PropertyValueFactory property="dni" />
               </cellValueFactory>
            </TableColumn>
            
            <TableColumn text="nacimiento">
               <cellValueFactory>
                  <PropertyValueFactory property="anio" />
               </cellValueFactory>
            </TableColumn>
            
            <TableColumn text="email">
               <cellValueFactory>
                  <PropertyValueFactory property="email" />
               </cellValueFactory>
            </TableColumn>
            
            <TableColumn text="nºS.Social">
               <cellValueFactory>
                  <PropertyValueFactory property="noSS" />
               </cellValueFactory>
            </TableColumn>
            
            <TableColumn text="teléfono">
               <cellValueFactory>
                  <PropertyValueFactory property="telefono" />
               </cellValueFactory>
            </TableColumn>
            
            
            <TableColumn text="datos">
               <cellValueFactory>
                  <PropertyValueFactory property="datos" />
               </cellValueFactory>
            </TableColumn>
            
            <TableColumn text="género">
               <cellValueFactory>
                  <PropertyValueFactory property="generos" />
               </cellValueFactory>
            </TableColumn>
            <TableColumn prefWidth="75.0" text="altura" />
            <TableColumn prefWidth="75.0" text="peso" />
            
         </columns>
      </TableView>
        <Button fx:id="secondaryButton" onAction="#mostrarPantalla" text="Volver">
         <graphic>
              <Label />
         </graphic></Button>
      <Button fx:id="grafica" onAction="#mostrarGrafica" text="Grafica">
         <graphic>
            <Label />
         </graphic>
      </Button>
      
        <Button fx:id="btnPDF" layoutX="676.0" layoutY="523.0" mnemonicParsing="false" onAction="#generarPDF" text="Generar PDF" />
      
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
