<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="568.0" prefWidth="979.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mass.ejercicionavegabilidad.ControladorPantallaB">
    <children>
   <TableView fx:id="tablaPacientes" layoutX="11.0" layoutY="275.0" prefHeight="339.0" prefWidth="939.0">
         <columns>
            
             <TableColumn text="id">
               <cellValueFactory>
                  <PropertyValueFactory property="id" />
               </cellValueFactory>
            </TableColumn>
            
            <TableColumn text="nombre">
               <cellValueFactory>
                  <PropertyValueFactory property="nombre" />
               </cellValueFactory>
            </TableColumn>
            
            <TableColumn text="apellido">
               <cellValueFactory>
                  <PropertyValueFactory property="apellido" />
               </cellValueFactory>
            </TableColumn>
            
                  <TableColumn text="dni">
               <cellValueFactory>
                  <PropertyValueFactory property="dni" />
               </cellValueFactory>
            </TableColumn>
            
            <TableColumn text="nacimiento">
               <cellValueFactory>
                  <PropertyValueFactory property="anio" />
               </cellValueFactory>
            </TableColumn>
            
            <TableColumn text="email">
               <cellValueFactory>
                  <PropertyValueFactory property="email" />
               </cellValueFactory>
            </TableColumn>
            
            <TableColumn text="nºS.Social">
               <cellValueFactory>
                  <PropertyValueFactory property="noSS" />
               </cellValueFactory>
            </TableColumn>
            
            <TableColumn text="teléfono">
               <cellValueFactory>
                  <PropertyValueFactory property="telefono" />
               </cellValueFactory>
            </TableColumn>
            
            
            <TableColumn text="datos">
               <cellValueFactory>
                  <PropertyValueFactory property="datos" />
               </cellValueFactory>
            </TableColumn>
            
            <TableColumn text="género">
               <cellValueFactory>
                  <PropertyValueFactory property="generos" />
               </cellValueFactory>
            </TableColumn>
            <TableColumn prefWidth="75.0" text="altura" />
            <TableColumn prefWidth="75.0" text="peso" />
            
         </columns>
      </TableView>
      <HBox prefHeight="90.0" prefWidth="763.0">
         <children>
            
              <Button fx:id="btnPDF" mnemonicParsing="false" onAction="#generarPDF" prefHeight="27.0" prefWidth="93.0" style="-fx-background-radius: 100;" text="Generar PDF">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
            <Button fx:id="grafica" onAction="#mostrarGrafica" prefHeight="27.0" prefWidth="93.0" style="-fx-background-radius: 100;" text="Grafica">
               <graphic>
                  <Label />
               </graphic>
               <HBox.margin>
                  <Insets left="25.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="btnLog" mnemonicParsing="false" onAction="#mostrarLog" prefHeight="27.0" prefWidth="93.0" style="-fx-background-radius: 100;" text="Log">
               <HBox.margin>
                  <Insets left="25.0" />
               </HBox.margin>
            </Button>
              <Button fx:id="secondaryButton" onAction="#mostrarPantalla" prefHeight="27.0" prefWidth="93.0" style="-fx-background-radius: 100;" text="Volver">
               <graphic>
                    <Label />
               </graphic>
               <HBox.margin>
                  <Insets left="25.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <fx:include source="Reloj.fxml" />
      
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
